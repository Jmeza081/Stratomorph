{
  "name": "stratomorph",
  "version": "0.0.1",
  "description": "stratomorph CLI",
  "private": true,
  "bin": {
    "stratomorph": "bin/stratomorph"
  },
  "scripts": {
    "format": "prettier --write **/*.{js,json} && standard --fix",
    "lint": "standard",
    "test": "jest",
    "watch": "jest --watch",
    "snapupdate": "jest --updateSnapshot",
    "coverage": "jest --coverage"
  },
  "files": [
    "LICENSE",
    "readme.md",
    "docs",
    "bin",
    "src"
  ],
  "license": "MIT",
  "dependencies": {
    "canvas": "^2.2.0",
    "color": "^3.1.0",
    "gluegun": "next",
    "rgbquant": "^1.1.2"
  },
  "devDependencies": {
    "jest": "^23.6.0",
    "prettier": "^1.12.1",
    "standard": "^12.0.1"
  },
  "jest": {
    "testEnvironment": "node"
  },
  "standard": {
    "env": [
      "jest"
    ]
  },
  "prettier": {
    "semi": false,
    "singleQuote": true
  }
}
